<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>8.0.0</Version> 
    <Authors>Juho Vainio, Mikko Kunnari</Authors>
    <Company>Fellowmind Oy Ab</Company>
    <Description>Helper library for Microsoft.OData.Client that provides generic methods for working with CE and OPS entities.</Description>
    <PackageTags>odata</PackageTags>
    <Copyright>Fellowmind Oy Ab</Copyright>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes></PackageReleaseNotes>
    <LangVersion>7.3</LangVersion>
